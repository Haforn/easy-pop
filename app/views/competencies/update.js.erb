$('.competency-detail').html('').append('<%= j render("competency_detail") %>');
$('.js-competency').find('a').text('<%= @competency.title %>')

removePopUp();

function removePopUp() {
	if ($('.pop-up').hasClass('active')) {
		$('.pop-up, .overlay').removeClass('active');
		setTimeout(function() {
			$('.pop-up > .container').html('');
		}, 300);
	}	
}